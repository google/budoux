{
  "name": "budoux-cli",
  "version": "0.0.1",
  "description": "A node cli tool of budoux.",
  "repository": {
    "type": "git",
    "url": "https://github.com/google/budoux.git",
    "directory": "javascript"
  },
  "bin": {
    "budoux-cli": "bin/budoux.js"
  },
  "scripts": {
    "build": "tsc --outDir dist --module CommonJS",
    "prepare": "npm run build",
    "test": "npm run build && npm link && ts-node node_modules/jasmine/bin/jasmine tests/*.ts",
    "lint": "gts lint",
    "fix": "gts fix",
    "clean": "rm -rf dist module"
  },
  "author": "Shuhei Iitsuka",
  "license": "Apache-2.0",
  "devDependencies": {
    "@types/jasmine": "^3.9.1",
    "@types/node": "^14.11.2",
    "@types/yargs": "^17.0.7",
    "gts": "^3.1.0",
    "jasmine": "^3.9.0",
    "ts-node": "^10.2.1",
    "typescript": "^4.3.5"
  },
  "dependencies": {
    "budoux": "^0.0.1",
    "yargs": "^17.2.1"
  }
}
